{
  "name": "@nosana/cli",
  "version": "0.0.0",
  "description": "",
  "bin": {
    "nosana": "./dist/src/index.js"
  },
  "type": "module",
  "scripts": {
    "dev": "APP_ENV=development node --no-warnings --loader ts-node/esm src/index.ts",
    "build": "npm run prepare && tsc && cp .env ./dist/ && cp .env.prd ./dist/ && cp .env.dev ./dist/",
    "generate": "openapi-typescript http://dashboard.k8s.prd.nos.ci/api/swagger/json -o ./src/api/schema.d.ts",
    "test": "APP_ENV=development DOCKER_CERT_PATH= vitest run",
    "ci:test": "APP_ENV=development DOCKER_CERT_PATH= vitest run --coverage --reporter=junit --outputFile=./test-report.xml",
    "test:watch": "APP_ENV=development DOCKER_CERT_PATH= vitest",
    "test:coverage": "APP_ENV=development DOCKER_CERT_PATH= vitest run --coverage",
    "publish:next": "npm run build && npm publish --tag=next --access public",
    "publish:public": "npm run build && npm publish --access public",
    "prepare": "npm run generate && ts-patch install && typia patch",
    "prettier": "prettier --check ./src",
    "prettier:fix": "prettier --write ./src",
    "eslint": "eslint ./src",
    "eslint:fix": "eslint --fix ./src",
    "lint": "npm run prettier && npm run eslint",
    "lint:fix": "npm run prettier:fix && npm run eslint:fix"
  },
  "keywords": [],
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json",
    "npm-shrinkwrap.json"
  ],
  "author": "Laurens Verspeek",
  "license": "ISC",
  "devDependencies": {
    "@ryoppippi/unplugin-typia": "^2.6.5",
    "@types/bn.js": "^5.1.6",
    "@types/cli-progress": "^3.11.6",
    "@types/cors": "^2.8.17",
    "@types/dockerode": "^3.3.23",
    "@types/eventsource": "^1.1.15",
    "@types/express": "^4.17.21",
    "@types/figlet": "^1.5.6",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^20.3.2",
    "@types/shell-quote": "^1.7.5",
    "@types/tar": "^6.1.6",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@vitest/coverage-v8": "^4.0.7",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^9.1.7",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.2",
    "vite": "^7.2.4",
    "vitest": "^4.0.7",
    "vitest-mock-extended": "^3.1.0"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.28.1-beta.1",
    "@inquirer/prompts": "^4.3.0",
    "@nosana/sdk": "0.4.73",
    "@solana/web3.js": "^1.78.0",
    "bn.js": "^5.2.1",
